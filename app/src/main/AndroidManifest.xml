<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.udemy.sevenminutesworkout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:name=".WorkOutApp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SevenMinutesWorkOut">
        <activity
            android:name="com.udemy.sevenminutesworkout.ui.view.HistoryActivity"
            android:exported="false" />
        <activity
            android:name="com.udemy.sevenminutesworkout.ui.view.BMIActivity"
            android:exported="false" />
        <activity
            android:name="com.udemy.sevenminutesworkout.ui.view.FinishActivity"
            android:exported="false" />
        <activity
            android:name="com.udemy.sevenminutesworkout.ui.view.ExerciseActivity"
            android:exported="false" />
        <activity
            android:name="com.udemy.sevenminutesworkout.ui.view.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>